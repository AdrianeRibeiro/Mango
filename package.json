{
  "name": "mango",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config dist/main",
    "dev": "ts-node-dev --respawn --transpile-only --clear -r dotenv/config --inspect=0.0.0.0:9229 src/main",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig-build.json",
    "prepare": "husky install",
    "check": "npm-check -s -u",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm t -- --watch",
    "test:staged": "npm t -- --findRelatedTests",
    "test:coverage": "npm t -- --coverage",
    "test:integration": "npm t -- --config ./jest.integration.config.js",
    "test:coveralls": "npm run test:coverage && coveralls < coverage/lcov.info",
    "test:fb-api": "npm run test:integration -- ./tests/external/facebook-api.test.ts",
    "test:s3": "npm run test:integration -- ./tests/external/aws-s3-file-storage.test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest-mock/express": "^2.0.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/module-alias": "^2.0.2",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.8.4",
    "@types/supertest": "^2.0.14",
    "@types/uuid": "^9.0.5",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "mockdate": "^3.0.5",
    "pg-mem": "^2.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.3",
    "aws-sdk": "^2.1472.0",
    "axios": "^1.5.1",
    "cors": "^2.8.5",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "npm-check": "^6.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  }
}
